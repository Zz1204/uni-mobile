{
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "@dcloudio/types",
      "@uni-helper/uni-app-types",
      "./components.d.ts",
      "./auto-imports.d.ts",
      "./uni-pages.d.ts"
    ]
  },
  "vueCompilerOptions": {
    "nativeTags": ["block", "component", "template", "slot"]
  }
}
